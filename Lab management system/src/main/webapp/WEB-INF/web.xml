<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SelectStudentInfo</servlet-name>
        <servlet-class>com.example.management113a.web.SelectStudentInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SelectStudentInfo</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SelectStudentTime</servlet-name>
        <servlet-class>com.example.management113a.web.SelectStudentTime</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SelectStudentTime</servlet-name>
        <url-pattern>/select</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AddStudentTime</servlet-name>
        <servlet-class>com.example.management113a.web.AddStudentTime</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddStudentTime</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
</web-app>